<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
"http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
		<class name="com.AmanoraDurga.Model.Admin.FlatType" table="FlatType" catalog="AmanoraDurgaDb">
			<id name="id" type="java.lang.Integer">
			<column name="id"/>
			<generator class="identity"/> <!--  // when to use <generator class="identity"></generator> and <generator class="identity"/> -->
			</id>
			<property name="code" type="string"> 
				<column name="code" length="12" not-null="true" unique="true"/>
			</property>
			<property name="type" type="string"> 
				<column name="type" length="255" not-null="true" unique="true"/>
			</property>
		</class>
</hibernate-mapping>

